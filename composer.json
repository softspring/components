{
    "name": "softspring/components",
    "description": "This library provides some Symfony components like templates.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^1.9"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\Components\\": "src/"
        }
    }
}
